@article{EMC_ext,
	title = "Measurement of the ratios of deep inelastic muon-nucleus cross sections on various nuclei compared to deuterium",
	journal = "Physics Letters B",
	volume = "202",
	number = "4",
	pages = "603 - 610",
	year = "1988",
	issn = "0370-2693",
	doi = "https://doi.org/10.1016/0370-2693(88)91872-2",
	url = "http://www.sciencedirect.com/science/article/pii/0370269388918722",
	author = "J. Ashman and others",
	abstract = "Results are presented on the ratios of the deep inelastic muon-nucleus cross sections for carbon, copper and tin nuclei to those measured on deuterium. The data confirm that the structure functions of the nucleon measured in nuclei are different from those measured on quasi-free nucleons in deuterium. The kinematic range of the data is such that 〈Q2〉 ∼ 5 GeV2 at x ∼ 0.03, increasing to 〈Q2〉 ∼ 35 GeV2 for x ∼ 0.65. The measured cross section ratios are less than unity for x ≲ 0.05 and for 0.25 ≲ x < 0.7. The decrease of the ratio below unity for low x becomes larger as A increases as might be expected from nuclear shadowing. However, this occurs at relatively large values of Q2 (∼ 5 GeV2) indicating that such shadowing is of patrionic origin."
}

